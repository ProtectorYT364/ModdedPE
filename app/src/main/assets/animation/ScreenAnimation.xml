<!-- don't touch unless you are an animator! -->
<maas class="com.microsoft.xbox.xle.anim.XLEMAASAnimationPackageNavigationManager">
    <forward>
        <inAnimation>
            <animations>
                <XLEAnimationDefinition durationMs="833" easing="EaseOut" from="0.6"
                    parameter="7" property="Scale" to="1" type="Exponential" />
                <XLEAnimationDefinition durationMs="333" from="0" property="Alpha" to="1"
                    type="Linear" />
            </animations>
        </inAnimation>
        <outAnimation>
            <animations>
                <XLEAnimationDefinition durationMs="666" easing="EaseIn" from="1" parameter="7"
                    property="Scale" to="1.2" type="Exponential" />
                <XLEAnimationDefinition delayMs="333" durationMs="333" from="1" property="Alpha" to="0"
                    type="Linear" />
            </animations>
        </outAnimation>
    </forward>
    <backward>
        <inAnimation>
            <animations>
                <XLEAnimationDefinition durationMs="633" easing="EaseOut" from="1.2"
                    parameter="7" property="Scale" to="1" type="Exponential" />
                <XLEAnimationDefinition delayMs="633" from="0" property="Alpha" to="1"
                    type="Linear" />
            </animations>
        </inAnimation>
        <outAnimation>
            <animations>
                <XLEAnimationDefinition durationMs="666" easing="EaseIn" from="1" parameter="7"
                    property="Scale" to="0.6" type="Exponential" />
                <XLEAnimationDefinition delayMs="333" durationMs="333" from="1" property="Alpha"
                    to="0" type="Linear" />
            </animations>
        </outAnimation>
    </backward>
</maas>
