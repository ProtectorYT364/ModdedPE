<!-- don't touch unless you are an animator! -->
<maas class="com.microsoft.xbox.xle.anim.XLEMAASAnimationPackageNavigationManager">
    <forward>
        <inAnimation>
            <animations>
                <XLEAnimationDefinition durationMs="150" easing="EaseOut" from="1.2"
                    parameter="7" property="Scale" to="1" type="Exponential" />
                <XLEAnimationDefinition delayMs="50" durationMs="100" easing="EaseOut" from="0"
                    property="Alpha" to="1" type="Linear" />
            </animations>
        </inAnimation>
        <outAnimation>
            <animations>
                <XLEAnimationDefinition durationMs="150" easing="EaseIn" from="1" parameter="7"
                    property="Scale" to="1.2" type="Exponential" />
                <XLEAnimationDefinition delayMs="50" durationMs="100" easing="EaseOut" from="1"
                    property="Alpha" to="0" type="Linear" />
            </animations>
        </outAnimation>
    </forward>
    <backward>
        <inAnimation>
            <animations>
                <XLEAnimationDefinition durationMs="666" easing="EaseOut" from="1.2"
                    parameter="7" property="Scale" to="1" type="Exponential" />
                <XLEAnimationDefinition delayMs="166" durationMs="500" easing="EaseInOut" from="0"
                    property="Alpha" to="1" type="Linear" />
            </animations>
        </inAnimation>
        <outAnimation>
            <animations>
                <XLEAnimationDefinition durationMs="666" easing="EaseIn" from="1" parameter="7"
                    property="Scale" to="0.79" type="Exponential" />
                <XLEAnimationDefinition delayMs="166" durationMs="500" easing="EaseInOut" from="1"
                    property="Alpha" to="0" type="Linear" />
            </animations>
        </outAnimation>
    </backward>
</maas>